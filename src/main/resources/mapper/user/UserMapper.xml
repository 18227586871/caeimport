<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mybatis.dao.UserMapper">
	<select id="getAllUser" parameterType="com.example.bean.UserBean" resultType="com.example.bean.UserBean">
	 	select * from user
	</select>
	<insert id="insertUser" parameterType="com.example.bean.UserBean">
		INSERT INTO USER (id, name, age) VALUES (#{id}, #{name}, #{age})
	</insert>
</mapper>